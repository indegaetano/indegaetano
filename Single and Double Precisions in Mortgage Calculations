    {
      "cell_type": "code",
      "source": [
        "def loan_amortization_single_precision(principle, rate, years):\n",
        "  # single precision is the 23 bits, I used the np.float32 for this on every variable\n",
        "  #principle = 500000\n",
        "  #rate = 4.25\n",
        "  # years = 30\n",
        "\n",
        "  amount_of_payments= years * 12\n",
        "  principle = np.float32(principle)\n",
        "  rate = np.float32(rate)\n",
        "  years = np.float32(years)\n",
        "  n = np.float32(12)\n",
        "  r = np.float32(rate / (100 *n ))\n",
        "\n",
        "\n",
        "  # Monthly Payments from the formmula\n",
        "  monthly_payments = np.float32((r * np.float32(principle)) / (1- (1+r)**(-(n*years))))\n",
        "\n",
        "  # Using the monthly payments we can calculate the total amount payed\n",
        "  total_payments = np.float32(monthly_payments) * np.float32(years) * 12\n",
        "\n",
        "  # Calculate the total Interest payed\n",
        "  total_interest= np.float32(total_payments) - np.float32(principle)\n",
        "\n",
        "  # Calculate the interest that is payed monthly\n",
        "\n",
        "  monthly_interest= np.float32(principle) * np.float32(r)\n",
        "\n",
        "  # Now we need to figure out the principle that is payed\n",
        "  monthly_principle = np.float32(monthly_payments) - np.float32(monthly_interest)\n",
        "\n",
        "  # print(monthly_payments, monthly_interest, monthly_principle)\n",
        "\n",
        "  for n in range(1,amount_of_payments):\n",
        "    while principle > monthly_principle:\n",
        "      new_monthly_interest= np.float32(principle) * np.float32(r)\n",
        "      new_monthly_principle= np.float32(monthly_payments) - np.float32(new_monthly_interest)\n",
        "      principle= np.float32(principle) - np.float32(new_monthly_principle)\n",
        "\n",
        "      print(\"Number of payments: \", n, f\"The Interest payed:  $ {new_monthly_interest:.2f}\",\n",
        "            f\" Monthly Principle payed is: $ {new_monthly_principle:.2f}\", f\" The remaining Principle is: $ {principle:.2f}\" )\n",
        "      n = n+1\n",
        "      if new_monthly_principle <= 0:\n",
        "        break\n",
        "\n",
        "  return monthly_payments, total_payments, total_interest, monthly_interest, monthly_principle\n",
        "\n",
        "loan_amortization_single_precision(500000, 4.25, 30)\n"
      ],
